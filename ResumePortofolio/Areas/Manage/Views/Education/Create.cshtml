@model Education
@{
    ViewData["Title"] = "Create";
}
   
<div class="row">
    <div class="col-xl-12">
        <!-- Product Info Start -->
        <div class="mb-5">
            <div class="card">
                <div class="card-body">
                    <form method="post">
                        <div class="mb-3">
                            <label asp-for="SubTitle" class="form-label"></label>
                            <input asp-for="SubTitle" class="form-control"/>
                            <span asp-validation-for="SubTitle"></span>
                        </div>
                        <div class="mb-3">
                            <label asp-for="Title" class="form-label"></label>
                            <input asp-for="Title" class="form-control" />
                            <span asp-validation-for="Title"></span>
                        </div>
                        <div class="mb-3">
                            <label asp-for="Info" class="form-label"></label>
                            <input asp-for="Info" class="form-control"/>
                            <span asp-validation-for="Info" class="text-primary"></span>
                        </div>
                        <div class="mb-3">
                            <label asp-for="dateTime" class="form-label"></label>
                            <input asp-for="dateTime" class="form-control"/>
                            <span asp-validation-for="dateTime" class="text-primary"></span>
                        </div>
                       
                        <div class="mb-3">
                            <input  type="submit" class="btn btn-outline-success" value="Create" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
